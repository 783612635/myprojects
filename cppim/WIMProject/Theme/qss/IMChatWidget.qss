/*滚动条样式表*/
QScrollBar{
    background:#FFFFFF;
    width:10px;
    padding:0px;
    border-radius:5px;
}
QScrollBar::handle{
    background:#BDBDBD;
    width:10px;
    border-radius:5px;
}
QScrollBar::handle:hover{
    background:#9E9E9E;
    width:10px;
    border-radius:5px;
}
QScrollBar::handle:pressed{
    background:#757575;
    width:10px;
    border-radius:5px;
}
