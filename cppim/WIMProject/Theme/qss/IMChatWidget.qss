QWidget#IMChatWidget
{
    border-radius:5px;
    border:1px solid #ff0000;
}
QWidget[userPhoto=true]
{
    background:white;
}
/*姓名按钮样式*/
QPushButton#pbt_userName
{
    border:none;
    font-family: "Microsoft YaHei";
    font-size:24px;
}
/*发送输入框样式表*/
QTextEdit#textEdit_send
{
    border:none;
    background:transparent;
}
/*用户名称*/
QPushButton#pbt_userName:hover
{
    border:none;
    font-family: "Microsoft YaHei";
    font-size:24px;
    border-bottom:2px solid #000000;
}

